/**
 * Definition for singly-linked list.
 * public class ListNode {
 *     int val;
 *     ListNode next;
 *     ListNode(int x) {
 *         val = x;
 *         next = null;
 *     }
 * }
 */
public class Solution {
    HashSet<ListNode> count = new HashSet<ListNode>();
    public ListNode getIntersectionNode(ListNode headA, ListNode headB) {
        while(headA != null){
            count.add(headA);
            headA = headA.next;
        }
        while(headB != null){
            if (count.contains(headB)){
                return headB;
            }
            headB = headB.next;
        }
        return null;
    }
}

/*
Notes:

(Using “HashSet” to count visited ListNodes.)
*/
