class Solution {
    public int[] prisonAfterNDays(int[] cells, int n) {
        int circle = (n - 1) % 14 + 1;
        for (int i = 0; i < circle; i++){
            int[] temp = new int[cells.length];
            for (int j = 1; j < cells.length - 1; j++){
                if (cells[j - 1] == cells[j + 1]){
                    temp[j] = 1;
                } else{
                    temp[j] = 0;
                }
            }
            cells = Arrays.copyOf(temp, 8);
        }
        return cells;
    }
}
